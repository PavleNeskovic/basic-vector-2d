  "author": "PavleNeskovic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PavleNeskovic/basic-vector-2d/issues"
  },
  "homepage": "https://github.com/PavleNeskovic/basic-vector-2d#readme",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "jest": "^26.4.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "lib/**/*"
  ]
